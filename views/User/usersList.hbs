{{#if alert}}
<div class="alert alert-success alert-dismissible">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <strong>Success!</strong> {{alert}}
</div>
{{/if}}
<ul class="breadcrumb">
  <li class="breadcrumb-item"><a href="/">Лист заявок</a></li>
  <li class="breadcrumb-item active">{{title}}</li>
</ul> 
<table class="table table-hover">
    <thead>
        {{#if userslist}}
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>User Description</th>
            <th>Password</th>
            <th>Role</th>
            <th>OrgUnity</th>
        </tr>
        {{/if}}
    </thead>
    <tbody>
        {{#each userslist}}
        <a href="">
            <tr>
                <td>{{_id}}</td>
                <td>{{userName}}</td>
                <td>{{userDescription}}</td>
                <td>{{userPassword}}</td>
                <td>{{userRole}}</td>
                <td>{{userOrgUnity}}</td>
                <td>
                    <a href="/users/edit?id={{_id}}">Edit</a>
                </td>
            </tr>
        </a>
        {{/each}}
    </tbody>
</table>